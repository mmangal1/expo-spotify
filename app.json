{
  "expo": {
    "name": "Expo Spotify",
    "description": "spotify ui clone with expo",
    "scheme": "spotifyclone",
    "slug": "spotifyclone",
    "privacy": "public",
    "version": "0.0.1",
    "platforms": [
      "android",
      "ios",
      "web"
    ],
    "githubUrl": "https://github.com/calebnance/expo-spotify",
    "orientation": "portrait",
    "primaryColor": "#181414",
    "icon": "src/assets/icon.png",
    "splash": {
      "backgroundColor": "#181414",
      "image": "src/assets/splash.png",
      "resizeMode": "contain"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "android": {
      "versionCode": 1,
      "package": "android.pkg"
    },
    "ios": {
      "buildNumber": "0.0.1",
      "supportsTablet": true,
      "bundleIdentifier": "com.ios.maikup"
    }
  }
}
